{
  "expo": {
    "name": "Guatemala Visible",
    "slug": "task-list-project-gv",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-guatemala-visible.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.luiscarrera.guatemalavisible",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-guatemala-visible.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.luiscarrera.guatemalavisible"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c5336946-2841-4799-a74a-95682c2df880"
      }
    }
  }
}
